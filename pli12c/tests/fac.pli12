# This program computes n!.

function main() returns int
begin
	declare k int;
	declare f int;

	write "Please input N: ";
	read k;

	write k;
	write "! is ";
	write fac(k);
	write "\n";

	return 0;
end

function fac(n: int) returns int
begin
	if (n <= 1) then
		return 1;
	else
		return n * fac(n);
	endif
end
