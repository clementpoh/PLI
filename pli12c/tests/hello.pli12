function cow(n: int) returns int
begin
    declare y int initialize to 3;

    y := y + n;

    while (n > 0) do 
        read n;
    endwhile

    if (n > 10) then
        write n;
    else
        if true then
            write "cow";
        endif 
    endif

    return y;
end

