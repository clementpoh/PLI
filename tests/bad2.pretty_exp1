function f(x: int, y: int) returns int
begin
    declare x int;
    declare z real;
    declare z real;

    read z;
    if z + 5.000000 then
        while z / 2 do
            write z;
            z := z + 1;
        endwhile
    else
        write 0.000000;
    endif
    return trunc(z);
end

